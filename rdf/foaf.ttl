@prefix clariah: <http://qber.data2semantics.org/vocab/clariah/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix organization: <http://qber.data2semantics.org/vocab/clariah/organizations/> .
