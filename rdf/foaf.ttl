clariah:CLARIAH a org:OrganizationalCollaboration ;
    org:hasMember organization:IISG,
        organization:UU,
        organization:VU ;
    foaf:homepage <http://www.clariah.nl/> ;
    foaf:name "Common Lab Research Infrastructure for the Arts and Humanities" .

organization:IISG a foaf:Agent ;
    rdfs:subClassOf org:Organization ;
    skos:label "International Institute of Social History" ;
    org:Site "Cruquiusweg 31 1019AT Amsterdam The Netherlands" ;
    org:hasMember people:richard,
        people:siyang ;
    foaf:homepage <https://socialhistory.org/> .

organization:UU a foaf:Agent ;
    rdfs:subClassOf org:Organization ;
    skos:label "Universiteit Utrecht" ;
    org:hasMember people:auke .

organization:VU a foaf:Agent ;
    rdfs:subClassOf org:Organization ;
    skos:label "Vrije Universiteit Amsterdam" ;
    org:hasMember people:kathrin,
        people:rinke .

people:auke a foaf:Person ;
    foaf:firstname "Auke" ;
    foaf:homepage <https://nl.linkedin.com/pub/auke-rijpma/38/42b/128> ;
    foaf:member clariah:CSDH ;
    foaf:surname "Rijpma" .

people:kathrin a foaf:Person ;
