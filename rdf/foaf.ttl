    foaf:surname "Dentler" .

people:richard a foaf:Person ;
    foaf:firstname "Richard" ;
    foaf:homepage <https://nl.linkedin.com/in/richardzijdeman> ;
    foaf:member clariah:CSDH ;
    foaf:surname "Zijdeman" .

people:rinke a foaf:Person ;
    foaf:firstname "Rinke" ;
