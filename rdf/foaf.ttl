    foaf:member clariah:CSDH ;
    foaf:surname "Hoekstra" .

people:siyang a foaf:Person ;
    foaf:firstname "Siyang" ;
    foaf:homepage <https://nl.linkedin.com/pub/siyang-kong/82/36b/610> ;
